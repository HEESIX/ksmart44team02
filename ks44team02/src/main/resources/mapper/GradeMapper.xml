<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ks44team02.mapper.GradeMapper">
	<resultMap type="Grade" id="GradeResultMap">
		<id column="member_level_code" property="memberLevelCode"/>
		<result column="member_level_class_name" property="memberLevelClassName"/>
		<result column="member_level_name" property="memberLevelName"/>
		<result column="member_level_reg_date" property="memberLevelRegDate"/>
	</resultMap>

<!-- 구매자 등급 정의 리스트 -->
	<select id="getbuyerGradeList" parameterType="String" resultMap="GradeResultMap">
		SELECT
			member_level_code
			,member_level_class_name
			,member_level_name
			,member_level_reg_date
		from
			tb_member_level
		WHERE
			member_level_class_name='구매자';
	</select>
	
<!-- 판매자 등급 정의 리스트 -->
	<select id="getsellerGradeinfo" parameterType="String" resultMap="GradeResultMap">
		SELECT
			member_level_code
			,member_level_class_name
			,member_level_name
			,member_level_reg_date
		from
			tb_member_level
		WHERE
			member_level_class_name='판매자';
	</select>



